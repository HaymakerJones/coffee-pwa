{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "coffee-api",
      "urls": [
        "http://localhost:8081/api"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "timeout": 1000,
        "maxSize": 30,
        "maxAge": 3600000
      }
    }
  ]
}